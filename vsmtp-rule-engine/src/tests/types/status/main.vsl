#{
    connect: [
        rule "test_status" || {
            if sys::next() is sys::next()
            && sys::next() == sys::next()

            && sys::next() not sys::accept()
            && sys::next() != sys::accept()

            && sys::next().to_string() == "next"
            && sys::next().to_debug() == "next" {

                print(sys::next());
                print(sys::accept());
                print(sys::faccept());
                print(sys::deny());

                sys::accept()
            } else {
                sys::deny()
            }
        }
    ]
}
