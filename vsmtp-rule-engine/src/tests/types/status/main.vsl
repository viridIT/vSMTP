#{
    connect: [
        rule "test_status" || {
            if next() is next()
            && next() == next()

            && next() not accept()
            && next() != accept()

            && next().to_string() == "next"
            && next().to_debug() == "next" {

                print(next());
                print(accept());
                print(faccept());
                print(deny());

                accept()
            } else {
                deny()
            }
        }
    ]
}
