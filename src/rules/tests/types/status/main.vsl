run_when!(
    #{
        connect: [
            rule "test_status" || {
                if vsl::next() is vsl::next()
                && vsl::accept() is vsl::accept()
                && vsl::faccept() is vsl::faccept()
                && vsl::deny() is vsl::deny()

                && vsl::next() not vsl::accept()
                && vsl::next() not vsl::faccept()
                && vsl::next() not vsl::deny() {
                    print(vsl::next().to_string());
                    print(vsl::next().to_debug());
                    vsl::accept()
                } else {
                    vsl::deny()
                }
            }
        ]
    }
)