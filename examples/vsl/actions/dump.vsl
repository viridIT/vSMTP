#{
    preq: [
        action "dump raw email to disk" || {
            try {
		dump("tests/generated");
		next();
            } catch (e) {
		print(e);
		deny();
            }
        },

        rule "trailing" || accept(),
    ],

    postq: [
        action "dump parsed email to disk" || {
            try {
                dump("tests/generated");
		next();
            } catch (e) {
		print(e);
		deny();
            }
        },

        rule "trailing" || accept(),
    ]
}
