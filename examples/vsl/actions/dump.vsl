#{
    preq: [
        action "dump raw email to disk" || {
            vsl::dump(srv, ctx, "tests/generated");
        },

        rule "trailing" || vsl::accept(),
    ],

    postq: [
        action "dump parsed email to disk" || {
            vsl::dump(srv, ctx, "tests/generated");
        },

        rule "trailing" || vsl::accept(),
    ]
}
