#{
    preq: [
        action "dump raw email to disk" || {
            sys::dump(srv, ctx, "tests/generated");
        },

        rule "trailing" || sys::accept(),
    ],

    postq: [
        action "dump parsed email to disk" || {
            sys::dump(srv, ctx, "tests/generated");
        },

        rule "trailing" || sys::accept(),
    ]
}
